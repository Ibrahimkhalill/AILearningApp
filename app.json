{
  "expo": {
    "name": "aileariningapp",
    "slug": "aileariningapp",
    "scheme": "aileariningapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/main_logo.png",
    "newArchEnabled": true,
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "com.google.android.c2dm.permission.RECEIVE"
      ]
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "updates": {
      "enabled": false
    },
    "plugins": [
      "expo-font",
      "expo-localization",
      "react-native-video",
      "expo-document-picker"
    ],
    "extra": {
      "eas": {
        "projectId": "0e55761c-c8d8-40ac-b83c-0cb4e11caa0a"
      }
    },
    "owner": "aininja"
  }
}
